<%= content_tag :div, class: 'row' do %>
  <%= content_tag :h2, class: 'col-12' do %>
    Editing <%= @organization.name %>
  <% end %>
<% end %>


<%= content_tag :div, class: 'row' do %>
  <%= content_tag :div, class: 'col-12' do %>

    <% unless @error.is_a?(Hash) && @error['errorcode'] %>
      <%= render 'form', organization: @organization %>
    <% else %>
      <%= @error['message'] %>
    <% end %>

  <% end %>
<% end %>

